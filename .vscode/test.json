{
  "": [
        {
      "type": "cmake",
      "label": "configure",
      "command": "configure",
      "preset": "${command:cmake.activeBuildPresetName}",
      "targets": [
        "${command:cmake.buildTargetName}"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "type": "cmake",
      "label": "Build",
      "command": "build",
      "preset": "${command:cmake.activeBuildPresetName}",
      "targets": [
        "${command:cmake.buildTargetName}"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}